<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.agp.profile.adapters.MainActivity">


  <de.hdodenhof.circleimageview.CircleImageView
      android:id="@+id/roundPic"
      android:layout_width="175dp"
      android:layout_height="175dp"
      android:layout_marginStart="24dp"
      android:layout_marginLeft="24dp"
      android:layout_marginTop="24dp"
      android:layout_marginEnd="24dp"
      android:layout_marginRight="24dp"
      android:src="@drawable/stock_pic"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/Title" />

  <TextView
      android:id="@+id/Phone_Out"
      android:layout_width="300dp"
      android:layout_height="30dp"
      android:layout_marginStart="24dp"
      android:layout_marginLeft="24dp"
      android:layout_marginTop="24dp"
      android:layout_marginEnd="24dp"
      android:layout_marginRight="24dp"
      android:background="@drawable/back"
      android:clickable="true"
      android:hint="e.g. 444-444-4444"
      android:onClick="GetPhone"
      android:paddingLeft="5dp"
      android:paddingRight="5dp"
      android:text="phone"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/Name_Out" />

  <TextView
      android:id="@+id/Email_Out"
      android:layout_width="300dp"
      android:layout_height="30dp"
      android:layout_marginStart="24dp"
      android:layout_marginLeft="24dp"
      android:layout_marginTop="24dp"
      android:layout_marginEnd="24dp"
      android:layout_marginRight="24dp"
      android:background="@drawable/back"
      android:clickable="true"
      android:hint="e.g. johndoe@gmail.com"
      android:onClick="GetEmail"
      android:paddingLeft="5dp"
      android:paddingRight="5dp"
      android:text="Email"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.497"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/Phone_Out" />

  <TextView
      android:id="@+id/About_Out"
      android:layout_width="300dp"
      android:layout_height="120dp"
      android:layout_marginStart="24dp"
      android:layout_marginLeft="24dp"
      android:layout_marginTop="28dp"
      android:layout_marginEnd="24dp"
      android:layout_marginRight="24dp"
      android:background="@drawable/back"
      android:clickable="true"
      android:hint="e.g. I love walks."
      android:onClick="GetAbout"
      android:paddingLeft="5dp"
      android:paddingRight="5dp"
      android:text="About"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.497"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/Email_Out" />

  <TextView
      android:id="@+id/Name_Out"
      android:layout_width="300dp"
      android:layout_height="30dp"
      android:layout_below="@id/Phone_Out"
      android:layout_alignParentEnd="true"
      android:layout_alignParentRight="true"
      android:layout_marginStart="24dp"
      android:layout_marginLeft="24dp"
      android:layout_marginTop="64dp"
      android:layout_marginEnd="24dp"
      android:layout_marginRight="24dp"
      android:background="@drawable/back"
      android:clickable="true"
      android:hint="e.g. John Doe"
      android:includeFontPadding="true"
      android:onClick="GetName"
      android:paddingLeft="5dp"
      android:paddingRight="5dp"
      android:text="name"
      android:textColor="#000000"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.497"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/roundPic" />

  <TextView
      android:id="@+id/textView4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Name"
      app:layout_constraintBottom_toTopOf="@+id/Name_Out"
      app:layout_constraintStart_toStartOf="@+id/Name_Out" />

  <TextView
      android:id="@+id/textView6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Phone"
      app:layout_constraintBottom_toTopOf="@+id/Phone_Out"
      app:layout_constraintStart_toStartOf="@+id/Phone_Out" />

  <TextView
      android:id="@+id/textView7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Email"
      app:layout_constraintBottom_toTopOf="@+id/Email_Out"
      app:layout_constraintStart_toStartOf="@+id/Email_Out" />

  <TextView
      android:id="@+id/textView8"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="About"
      app:layout_constraintBottom_toTopOf="@+id/About_Out"
      app:layout_constraintStart_toStartOf="@+id/About_Out" />

  <TextView
      android:id="@+id/Title"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="24dp"
      android:layout_marginLeft="24dp"
      android:layout_marginTop="24dp"
      android:layout_marginEnd="24dp"
      android:layout_marginRight="24dp"
      android:text="Edit Profile"
      android:textColor="@android:color/black"
      android:textSize="30sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>