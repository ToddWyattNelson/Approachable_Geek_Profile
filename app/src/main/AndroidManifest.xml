<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.approachablegeekprofile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.agp.profile.adapters.GetAboutActivity"
            android:parentActivityName="com.agp.profile.adapters.MainActivity"/>

        <activity android:name="com.agp.profile.adapters.GetEmailActivity"
            android:parentActivityName="com.agp.profile.adapters.MainActivity"/>

        <activity android:name="com.agp.profile.adapters.GetPhoneActivity"
            android:parentActivityName="com.agp.profile.adapters.MainActivity"/>

        <activity android:name="com.agp.profile.adapters.GetNameActivity"
            android:parentActivityName="com.agp.profile.adapters.MainActivity"/>

        <activity android:name="com.agp.profile.adapters.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>